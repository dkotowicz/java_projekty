package com.example.shdemo.service;

import java.util.List;

import com.example.shdemo.domain.Posrednik;
import com.example.shdemo.domain.Nieruchomosc;

public interface CRUDManager {
	
	void addPosrednik(Posrednik posrednik);
	void addNieruchomosc(Nieruchomosc nieruchomosc);
	
	void deletePosrednik(Posrednik posrednik);
	void deleteNieruchomosc(Nieruchomosc nieruchomosc);
	
	void updatePosrednik(Posrednik posrednik);
	void updateNieruchomosc(Nieruchomosc nieruchomosc);
	
	List<Posrednik> getAllPosrednik();
	List<Nieruchomosc> getAllNieruchomosc();
	
	Posrednik findPosrednikById(Long id);
	Posrednik findNieruchomoscById(Long id);
	
	void Nieruchomosc_Posrednik(Long posrednikId, Long nieruchomoscId);
	

}
